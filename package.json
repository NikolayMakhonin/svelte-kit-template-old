{
	"name": "svelte-kit-template",
	"version": "0.0.0",
	"description": "TODO",
	"type": "module",
	"engines": {
		"node": ">=16.15.1",
		"pnpm": "^7.5.0"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"lint": "eslint ./**/*.{js,cjs,mjs,ts,tsx,svelte,html}",
		"lint:fix": "eslint --fix ./**/*.{js,cjs,mjs,ts,tsx,svelte,html}",
		"lint:wizard": "eslint-nibble --cache --multi ./**/*.{js,cjs,mjs,ts,tsx,svelte,html}",
		"test": "mocha ./src/**/*.test.ts",
		"test:watch": "mocha --watch ./src/**/*.test.ts",
		"test:e2e:dev": "cross-env HOST=http://localhost:3000 mocha ./src/**/*.e2e.ts",
		"test:e2e:dev:debug": "cross-env HOST=http://localhost:3000 SHOW_BROWSER=true DELAY_ON_ERROR=600000 mocha ./src/**/*.e2e.ts",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch"
	},
