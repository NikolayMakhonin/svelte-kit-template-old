{
  "name": "svelte-kit-template",
  "version": "0.0.0",
  "description": "TODO",
  "type": "module",
  "engines": {
    "node": ">=16.15.1",
    "pnpm": "^7.5.0"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "prepare": "svelte-kit sync",
    "audit": "pnpm audit --prod",
    "lint": "eslint ./**/*.{js,cjs,mjs,ts,tsx,svelte,html} --no-error-on-unmatched-pattern",
    "lint:fix": "eslint --fix ./**/*.{js,cjs,mjs,ts,tsx,svelte,html} --no-error-on-unmatched-pattern",
    "lint:wizard": "eslint-nibble --cache --multi ./**/*.{js,cjs,mjs,ts,tsx,svelte,html}",
    "test": "cross-env HOST=http://localhost:3000 vitest run",
    "test:watch": "cross-env HOST=http://localhost:3000 vitest",
    "test:e2e:dev": "cross-env HOST=http://localhost:3000 vitest --ui .e2e.",
    "test:e2e:dev:debug": "cross-env HOST=http://localhost:3000 SHOW_BROWSER=true DELAY_ON_ERROR=600000 vitest --ui .e2e.",
    "install:playwright": "pnpm exec playwright install --with-deps",
    "test:ci": "pnpm run test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@babel/core": "7.18.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-classes": "7.18.4",
    "@babel/plugin-transform-runtime": "7.18.5",
    "@babel/preset-env": "7.18.2",
    "@babel/runtime-corejs3": "7.18.3",
    "@flemist/async-utils": "0.0.11",
    "@flemist/test-variants": "0.0.16",
    "@flemist/time-limits": "0.0.5",
    "@rollup/plugin-babel": "5.3.1",
    "@sveltejs/adapter-auto": "1.0.0-next.59",
    "@sveltejs/adapter-static": "1.0.0-next.34",
    "@sveltejs/kit": "1.0.0-next.372",
    "@types/assert": "1.5.6",
    "@types/express": "4.17.13",
    "@types/node": "18.0.0",
    "@types/serviceworker": "0.0.48",
    "@vitest/ui": "0.18.0",
    "cross-env": "7.0.3",
    "depcheck": "1.4.3",
    "dotenv": "16.0.1",
    "eslint": "8.18.0",
    "eslint-config-pro": "3.0.18",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-svelte3": "4.0.0",
    "express": "4.18.1",
    "fkill": "8.0.1",
    "fs-extra": "10.1.0",
    "rdtsc": "3.0.3",
    "rollup": "2.76.0",
    "svelte": "3.48.0",
    "svelte-check": "2.8.0",
    "svelte-preprocess": "4.10.7",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "url-join": "=4.0.1",
    "vite": "2.9.14",
    "vitest": "0.18.0"
  },
